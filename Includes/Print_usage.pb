Procedure print_usage()
  PrintN("USAGE: "+#PROGNAME+" [options] [script_without_extension].")
  PrintN("Available options are:")
  PrintN("  -d       force debug mode")
  PrintN("  -h       show this help")
  PrintN("  -v       show version information")
  PrintN("  -l       show licence information")
EndProcedure

